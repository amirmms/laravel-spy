{
    "name": "farayaz/laravel-spy",
    "description": "A Laravel package to track outgoing HTTP requests.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Farayaz\\LaravelSpy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Farayaz\\LaravelSpy\\LaravelSpyServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
