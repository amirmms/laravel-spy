{
  "name": "farayaz/laravel-spy",
  "description": "A Laravel package to track outgoing HTTP requests.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Farayaz\\LaravelSpy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Farayaz\\LaravelSpy\\LaravelSpyServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^8.0|^9.0",
    "mockery/mockery": "^1.4.4",
    "fakerphp/faker": "^1.9.1"
  },
  "scripts": {
    "test": "phpunit"
  }
}
